 	value = gpio_get_value(pdata->id_gpio) ? 1 : 0;
 
 	if (value)
-		set_irq_type(gpio_to_irq(pdata->id_gpio), IRQ_TYPE_LEVEL_LOW);
+		irq_set_irq_type(gpio_to_irq(pdata->id_gpio), IRQ_TYPE_LEVEL_LOW);
