 	if (pdata->id_gpio != 0) {
 		p_otg->fsm.id = gpio_get_value(pdata->id_gpio) ? 1 : 0;
 		if (p_otg->fsm.id)
-			set_irq_type(gpio_to_irq(pdata->id_gpio),
