+	pred = ((reg & MXC_CCM_CS2CDR_ENFC_CLK_PRED_MASK)
+		>> MXC_CCM_CS2CDR_ENFC_CLK_PRED_OFFSET) + 1;
+	podf = ((reg & MXC_CCM_CS2CDR_ENFC_CLK_PODF_MASK)
+		>> MXC_CCM_CS2CDR_ENFC_CLK_PODF_OFFSET) + 1;
