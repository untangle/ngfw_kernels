+	mux = _get_mux6(parent, &pll2_pfd_352M,
+		&pll2_528_bus_main_clk, &pll3_usb_otg_main_clk,
+		&pll2_pfd_400M, NULL, NULL);
+	reg |= mux << MXC_CCM_CS2CDR_ENFC_CLK_SEL_OFFSET;
