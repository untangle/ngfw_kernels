 	u64 tmp;
 
 	tmp = (u64)clk_get_rate(clk->parent) * 18;
+	tmp += rate/2;
 	do_div(tmp, rate);
 	frac = tmp;
-	frac = frac < 18 ? 18 : frac;
+	frac = frac < 12 ? 12 : frac;
 	frac = frac > 35 ? 35 : frac;
+	tmp = (u64)clk_get_rate(clk->parent) * 18;
 	do_div(tmp, frac);
 	return tmp;
 }
