 		(clk->parent == &ldb_di1_clk))
 		return clk_get_rate(clk->parent);
 
-	reg = __raw_readl(MXC_CCM_CHSCCDR);
+	reg = __raw_readl(MXC_CCM_CSCDR2);
 
-	div = (reg & MXC_CCM_CHSCCDR_IPU2_DI1_PODF_MASK) + 1;
